<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">

  <resource-ref>
    <res-ref-name>jms/ConnectionFactory</res-ref-name>
    <res-type>javax.jms.ConnectionFactory</res-type>
    <lookup-name>java:/ConnectionFactory</lookup-name>
  </resource-ref>
  <resource-ref>
    <res-ref-name>jms/queue/test</res-ref-name>
    <res-type>javax.jms.Queue</res-type>
    <lookup-name>java:/queue/test</lookup-name>
  </resource-ref>
  <resource-ref>
    <res-ref-name>jms/queue/testReply</res-ref-name>
    <res-type>javax.jms.Queue</res-type>
    <lookup-name>java:/queue/testReply</lookup-name>
  </resource-ref>

</web-app>
